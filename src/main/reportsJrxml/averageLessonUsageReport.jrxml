			<![CDATA[select flw.msisdn as "Msisdn",flw.operator as "Operator",flw.circle as "Circle",flw.name as "FLW Name",ld.district as "District",ld.block as "Block",ld.panchayat as "Panchayat",flw.status as "Status",flw.designation as "Designation",cd.name as "Course Name",round(avg(coalesce(c.duration,0)/1000.0),2) as "Avg Duration in Secs" from report.course_item_measure c,report.front_line_worker_dimension flw,report.location_dimension ld,report.course_item_dimension cd,report.time_dimension td where td.id = c.time_id and cd.parent_id in (select id from report.course_item_dimension where type = 'LESSON') and cd.id = c.course_item_id and c.flw_id=flw.id and c.location_id = ld.id and td.date >= $P{StartDate} and td.date <= $P{EndDate}  group by ld.district,ld.block,ld.panchayat,flw.msisdn,flw.operator,flw.name,flw.status,flw.designation,cd.content_id,cd.name,flw.circle]]>
		</queryString>
